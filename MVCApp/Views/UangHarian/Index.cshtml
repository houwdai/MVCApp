@model IEnumerable<MVCApp.Models.UangHarian>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nomor
            </th>
            <th>
                Provinsi
            </th>
            <th>
                Luar Kota
            </th>
            <th>
               Dalam Kota
            </th>
            <th>
                Diklat
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.provinsi)
            </td>
            <td>
                    Rp. @Html.DisplayFor(modelItem => item.luarkota)
            </td>
            <td>
                Rp. @Html.DisplayFor(modelItem => item.dalamkota)
            </td>
            <td>
                    Rp.@Html.DisplayFor(modelItem => item.diklat)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
